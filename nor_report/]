#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <unistd.h>
#include <math.h>


int main(){
  int coordinate_x;
  int coordinate_y;
  int coordinate_center_x;
  int coordinate_center_y;
  double length_side;
  double length_random;
  double length_circle;

  srand((unsigned int)time(NULL));

  length_side = 10;
  coordinate_x = rand() % (length_side + 1);
  coordinate_y = rand() % (length_side + 1);
  length_side = sqrt(pow(coordinate_x, 2) + pow(coordinate_y, 2));
  printf("%d, %d\n", coordinate_x, coordinate_y);
  printf("%d\n", length_side);

  return 0;
}
